# Root access to cluster nodes - uses xlence-id-ed25519 key
Match user root host xlence,xlence2,node*,ngs,licensebox,elenuar,licsrv,192.168.60.*,159.149.32.*
    IdentityFile ~/.ssh/xlence-id-ed25519
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host *
  ServerAliveInterval 60
  ForwardAgent yes
  # Compression yes

Host github.com
  Hostname github.com
  Port 22
  IdentityFile ~/.ssh/id_ed25519

